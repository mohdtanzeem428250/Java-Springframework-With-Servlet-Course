<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class="in.sp.beans.Student" id="student">
        <property name="name" value="Tanzeem"></property>
        <property name="age" value="36"></property>
        <property name="email" value="Tanzeem@gmail.com"></property>
    </bean>
    
     <bean class="in.sp.beans.Student" id="student1">
        <property name="name" value="Rahul"></property>
        <property name="age" value="21"></property>
        <property name="email" value="Rahul@gmail.com"></property>
    </bean>
    
     <bean class="in.sp.beans.Student" id="student2">
        <property name="name" value="Sohail"></property>
        <property name="age" value="24"></property>
        <property name="email" value="Sohail@gmail.com"></property>
    </bean>


</beans>